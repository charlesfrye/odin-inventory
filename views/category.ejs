<%- contentFor('body') %>
<div class="category mt-10">
  <p class="text-2xl"><%= category.name %></p>
  <p class="font-light mt-4"><%= category.description %></p>
  <ul class="item-list mt-6">
    <% items.forEach(function(item) { %>
    <li class="item mt-4">
      <a class="text-lg" href=<%=item.url %>><%= item.name %></a>
    </li>
    <% });%>
</div>
